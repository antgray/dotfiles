# color
alias ip='ip --color=auto'
alias ls='ls --color=auto'

# ls
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

# git
alias ga='git add *'
alias gc='git commit -m'
alias gs='git status'
alias gp='git push'
alias dot='/usr/bin/git --git-dir=/home/ag/.dotfiles/ --work-tree=/home/ag'

# misc
alias sshfix='ssh-keygen -f "/home/ag/.ssh/known_hosts" -R'
alias chmox='chmod +x'
alias c='clear'

